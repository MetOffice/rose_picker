  function model_depth_dimension(test_value_1, Test_value_2)

    implicit none

    integer(i_def), intent(in), optional  :: test_value_1, Test_value_2

    integer(i_def)                        :: upper_level, lower_level


    !> Set defaults if arguments not supplied
    if(present(test_value_1)) then
      lower_level = test_value_1
    else
      lower_level = SOME_TEST_VALUE
    endif

    if(present(Test_value_2)) then
      upper_level = Test_value_2
    else
      upper_level = SOME_OTHER_TEST_VALUE
    endif

  end function model_depth_dimension